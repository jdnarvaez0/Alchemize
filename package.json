{
  "name": "obsidian-alchemize",
  "version": "1.0.0",
  "description": "Alchemize: Transmuta contenido web en notas Markdown estructuradas en Obsidian",
  "main": "dist/main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs development",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "release": "bash scripts/release.sh"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "markdown",
    "html",
    "converter",
    "web-clipper",
    "alchemize",
    "study",
    "notes"
  ],
  "author": "Juan David Narvaez",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdnarvaez0/alchemize.git"
  },
  "bugs": {
    "url": "https://github.com/jdnarvaez0/alchemize/issues"
  },
  "homepage": "https://github.com/jdnarvaez0/alchemize#readme",
  "devDependencies": {
    "@mozilla/readability": "^0.4.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.0",
    "@types/turndown": "^5.0.4",
    "esbuild": "^0.19.8",
    "jsdom": "^23.0.1",
    "linkedom": "^0.16.4",
    "turndown": "^7.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "obsidian": "1.4.11"
  }
}